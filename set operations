
a= input("Enter elements of the set E (separated by space): ")
b = input("Enter elements of the set N (separated by space): ")
E = set(map(int, a.split()))
N = set(map(int, b.split()))

union_set = E | N
print(f"Union of E and N is {union_set}")

intersection_set = E & N
print(f"Intersection of E and N is {intersection_set}")

difference_set = E - N
print(f"Difference of E and N is {difference_set}")

symmetric_difference_set = E ^ N
print(f"Symmetric difference of E and N is {symmetric_difference_set}")
